  "dependencies": {
    "angular": "^1.6.10",
    "angular-route": "^1.6.10",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.1.1",
    "concurrently": "^3.5.1",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "node-sass": "^4.9.0",
    "passport": "^0.4.0",
